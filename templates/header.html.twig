<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('/assets/css/style.css') }}" />
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
        <link rel="icon" type="image/x-icon" href="{{ asset('/img/favicon.png') }}">
        <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />
        <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.css" />
        <script src="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js"></script>
        <script src="https://cdn.zingchart.com/zingchart.min.js"></script>
        <script src="https://cdn.zingchart.com/zingchart.jquery.min.js"></script>
    {% endblock %}
    <meta charset="UTF-8">
</head>
<body>
<div class="header">
    <div class="g-wrapper">
        <div class="header__wrapper-items">
            <a href="{{ path('app_menu_page') }}">
                <div class="header__wrapper-items__left">
                    <div class="logo-office"></div>
                    <div class="name_title">InfoOffice</div>
                </div>
            </a>
            <div class="header__wrapper-items__right">
                {%  if is_granted('IS_AUTHENTICATED_FULLY') %}
                   <a class="btn btn-white" href="{{ path('app_logout') }}">Выйти</a>
                {% else %}
                   <a class="btn btn-white" href="{{ path('app_register') }}">Регистрация</a>
                   <a class="btn btn-black" href="{{ path('app_login') }}">Войти</a>
                {% endif %}
            </div>
        </div>
    </div>
</div>

